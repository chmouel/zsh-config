#!/bin/bash
alias b='bzip2 -9vf'
alias z="gzip -9fv"
alias md='mkdir -p'

alias ls='ls --color=auto -F'
alias d=ls
alias s='cd ..'
alias p='cd -'
alias ll='ls -arSlh'
alias la='ls -A'
alias lt='ls -artl'

alias df='df -h'
alias du='du -sh'
alias cp="cp -i"
alias rm="rm -i"
alias mv="mv -i"

alias vih="vim ~/.shell/hosts/${HOST%%.*}.sh"

