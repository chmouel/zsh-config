local root=~/GIT/go/src/github.com/
local res results

if [[ -n $1 ]];then
    local results=($(fd -d 2 -t d . ${root}|gegrep "${1}"))
    [[ ${#results} == 1 ]] && { echo ${results}; cd ${results} ; return;}
    res=$(fd -d 2 -t d . ${root}|gegrep "${1}"|fzf)
    [[ -n ${res} ]] && cd ${res}
    return
fi

res=$(fd -d 2 -t d . ${root}|sed "s,${root},,"|fzf)
[[ -n ${res} ]] && cd ${root}${res}
